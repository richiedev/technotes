{
  "compilerOptions": {
    "jsx": "react",
    "module": "esnext",
    "outDir": "build/dist",
    "sourceMap": true,
    "strict": true,
    "target": "es5",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
